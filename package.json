{
  "name": "@tsklabs/ups-caz-templates",
  "version": "0.1.0",
  "private": true,
  "description": "A template for creating ups-caz-templates apps.",
  "keywords": [
    "ups-caz-templates",
    "tsklabs",
    "caz",
    "zce"
  ],
  "repository": "tsklabs/ups-caz-templates",
  "license": "MIT",
  "author": "lktslionel <lktslionel@gmail.com> (https://github.com/lktslionel)",
  "main": "index.js",
  "scripts": {
    "test": "standard --fix && jest"
  },
  "jest": {
    "testTimeout": 20000,
    "modulePathIgnorePatterns": [
      "<rootDir>/template"
    ]
  },
  "renovate": {
    "extends": [
      "zce"
    ],
    "ignorePaths": [
      "template"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "template"
    ]
  },
  "devDependencies": {
    "caz": "0.7.0",
    "jest": "27.0.6",
    "standard": "16.0.3"
  },
  "dependencies": {
    "@cucumber/cucumber": "^7.3.1"
  }
}
